  <!-- ************************ RealisedProduct **************************** -->
  <!-- -->
  <xsd:simpleType name="RealisedProductTraceableType" plm:deprecated="true">
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="repairable" plm:deprecated="true"/>
      <xsd:enumeration value="nonRepairable" plm:deprecated="true"/>
    </xsd:restriction>
  </xsd:simpleType>
  <!-- -->
  <xsd:complexType name="RealisedProductType">
    <xsd:annotation>
      <xsd:documentation>
      This represents a physical product, throughout its as-built/as-maintained
      lifecycle.

      Attributes:

      unitRef:          Inherited from Product. References a Unit that
                        represents the default unit of measure.
      manufacturerId:   Id (part number) assigned by Manufacturer.
      mrbId:            Id (part number) assigned by Material Review Board.
      manufactureDate:  Date (and time) of manufacture.
      manufacturerRef:  Ref to manufacturing Organisation.
      designRef:        Reference to Product Revision from which this product
                        was realised.
      standardLife:     Standard Life specified in given environment and
                        operation conditions.
      actualLife:       Actual Hours recorded by physical part.
      standardLifeValueRef: Reference to a ValueWithUnit giving the standard
                        life.
      actualLifeValueRef: Reference to a ValueWithUnit giving the actual life.
      lifeLimitedPart:  Whether the life of a part is limited.
      orderDate:        Physical Part Order Date.
      serialNumber:     Serial Number if a serialised product.
      lot:              Lot number if product is lot.
      traceablePartType: Whether the part is repairable or non-repairable.
      orderId:          Physical Part order id.
      defaultQuantity:  Default quantity.
      lotRef:           Reference to MRO lot.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:extension base="plm:ProductType">
        <xsd:attribute name="manufacturerId" type="xsd:string" use="optional" plm:deprecated="true"/>
        <xsd:attribute name="mrbId" type="xsd:string" use="optional" plm:deprecated="true"/>
        <xsd:attribute name="manufactureDate" type="xsd:dateTime" use="optional" plm:deprecated="true"/>
        <xsd:attribute name="manufacturerRef" type="plm:anyURIType" use="optional" plm:refType="plm:Organisation" plm:deprecated="true"/>
        <xsd:attribute name="designRef" type="plm:anyURIType" use="optional" plm:refType="plm:ProductRevision" plm:deprecated="true"/>
        <xsd:attribute name="standardLife" type="xsd:duration" use="optional" plm:deprecated="true"/>
        <xsd:attribute name="actualLife" type="xsd:duration" use="optional" plm:deprecated="true"/>
        <xsd:attribute name="standardLifeValueRef" type="plm:anyURIType" use="optional" plm:refType="plm:ValueWithUnit" plm:deprecated="true"/>
        <xsd:attribute name="actualLifeValueRef" type="plm:anyURIType" use="optional" plm:refType="plm:ValueWithUnit" plm:deprecated="true"/>
        <xsd:attribute name="lifeLimitedPart" type="xsd:boolean" use="optional" plm:deprecated="true"/>
        <xsd:attribute name="orderDate" type="xsd:dateTime" use="optional" plm:deprecated="true"/>
        <xsd:attribute name="serialNumber" type="xsd:string" use="optional" plm:deprecated="true"/>
        <xsd:attribute name="lot" type="xsd:string" use="optional" plm:deprecated="true"/>
        <xsd:attribute name="traceablePartType" type="plm:RealisedProductTraceableType" use="optional" plm:deprecated="true"/>
        <xsd:attribute name="orderId" type="xsd:string" use="optional" plm:deprecated="true"/>
        <xsd:attribute name="defaultQuantity" type="xsd:float" use="optional"/>
        <xsd:attribute name="lotRef" type="plm:anyURIType" use="optional" plm:refType="plm:Lot"/>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="RealisedProduct" type="plm:RealisedProductType" substitutionGroup="plm:Product"/>
