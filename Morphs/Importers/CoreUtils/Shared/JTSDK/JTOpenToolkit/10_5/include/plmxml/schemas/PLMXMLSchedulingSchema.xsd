  <!-- ********************** FixedCostAccrualEnum *********************** -->
  <!-- -->
  <xsd:simpleType name="FixedCostAccrualEnum">
    <xsd:restriction base="xsd:NMTOKEN">
      <xsd:enumeration value="start"/>
